           <listBibl> 
                    <bibl>Primeramente <measure unit="vol" quantity="1">un libro</measure> <title>llamado floretodes <persName>bernardo</persName></title>
                  <note type="condition" subtype="binding">con su cubierta negro</note></bibl>
              <bibl><measure unit="vol" quantity="1">un</measure> <title>sermonario</title> <author>de thomas de ylixicoz</author></bibl>
              <bibl><measure unit="vol" quantity="1">un libro</measure> de <title>sermones funebres</title> <author>de Santo ximiniano</author></bibl>
                  <bibl><measure unit="vol" quantity="1">otro libro</measure> de <title>sermones de mornis securs</title></bibl>
              <bibl><measure unit="vol" quantity="1">otro libro</measure> <title>de questionibus super Evangelia</title> <author>de turre Cremata</author></bibl>
              <bibl><measure unit="vol" quantity="1">otro libro</measure> de <title>sermones</title> <author>de S. Vicente</author></bibl>
              <bibl><measure unit="vol" quantity="2">dos tomos</measure> de <title>sermones</title> <author>del pepin</author></bibl>
                  <bibl><measure unit="vol" cert="unknown">otros</measure> <title>libros de disipulo</title></bibl>
                  <bibl><measure unit="vol" quantity="1">un</measure> <title>brevario</title> <note type="condition" subtype="age">de los nuebos</note></bibl>
           </listBibl>
            <list>
               <head>Vestidos</head>
                  <item>Iten una ropa de paño vurel</item>
                  <item>unas calças de burel</item>
                  <item>una ropa vieja</item>
                  <item>una cassa y su torre</item>
                  <item>una pieça de sys robas echo hyermo</item>
                  <item>una anca de pino</item>
